
<div id="bgndVideo" class="player" data-property="{videoURL:'https://www.youtube.com/watch?v=GquEnoqZAK0',containment:'body',autoPlay:true, mute:true, startAt:0, opacity:1}"></div>

<div class="loginbox">
  <p id="diaryram">Diaryram</p>
  <% if user_signed_in? %>
    <p><%= link_to "로그아웃", destroy_user_session_path, method: :delete, data: {confirm: "Are you sure?" }, class: 'btn btn-default'%></p>
    <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
      <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingTwo">
          <h4 class="panel-title">
            <a class="collapsed" role="button" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
          New Post
            </a>
          </h4>
        </div>
        <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingTwo">
          <div class="panel-body">
        <%= render partial: "posts/news" %>
          </div>
        </div>
      </div>
    </div>
  <% else %>
    <span><%= link_to "로그인", new_user_session_path, class: 'btn btn-default' %></span>
    <span><%= link_to "페이스북으로 로그인", user_facebook_omniauth_authorize_path, class: 'btn btn-default' %></span>

  <% end %>
  </div>
<script src="inc/jquery.mb.YTPlayer.js"></script>
